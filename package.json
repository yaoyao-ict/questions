{
  "name": "qna",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm-run-all --parallel open lint:watch test:watch",
    "open": "babel-node ./src/index.dev.js",
    "lint": "esw webpack.config.* src",
    "lint:watch": "esw webpack.config.* src -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "./node_modules/.bin/rimraf ./dist",
    "prebuild": "npm-run-all test lint clean-dist",
    "build:html": "babel-node ./src/build/buildHtml.js",
    "build": "npm run build:html & babel-node ./src/build/build.js",
    "postbuild": "babel-node ./src/index.prod.js"
  },
  "dependencies": {
    "acorn": "^6.0.0",
    "bluebird": "^3.5.2",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.2.1",
    "cookie-parser": "~1.4.3",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "jquery": "^3.3.1",
    "jss": "^9.8.7",
    "jss-plugin-default-unit": "^10.0.0-alpha.3",
    "mongoose": "^5.2.17",
    "morgan": "~1.9.0",
    "open": "0.0.5",
    "popper.js": "^1.14.6",
    "prop-types": "^15.6.2",
    "pug": "~2.0.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-jss": "^10.0.0-alpha.5",
    "react-loadable": "^5.5.0",
    "react-loading": "^2.0.3",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "async": "^2.6.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "cheerio": "^1.0.0-rc.2",
    "colors": "^1.3.3",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "eslint-watch": "^4.0.2",
    "file-loader": "^3.0.1",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "ncp": "^2.0.0",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.5",
    "react-test-renderer": "^16.7.0",
    "regenerator-runtime": "^0.13.1",
    "shelljs": "^0.8.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test.setup.js",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
